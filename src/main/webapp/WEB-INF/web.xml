    <servlet>
        <servlet-name>RootServlet</servlet-name>
        <servlet-class>io.stackunderflow.flow.ui.web.landingpage.LandingPageEndpoint</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RootServlet</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>
</web-app>
